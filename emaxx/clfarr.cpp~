#include <iostream>

using namespace std;

struct Query {
  int l, r, c;
};

const int N = 2e5;
Query query[N];
int parent[N], color[N];

int main() {

  int n, q;
  scanf("%d%d", &n, &q);
  for (int i = 0; i < n; ++i)
    parent[i] = i;
  for (int i = 0; i < q; ++i)
    scanf("%d%d%d", &query[i].l, &query[i].r, &query[i].c);

  for (int i = q - 1; i >= 0; i--) {
    int l, r, c;
    l = query[i].l - 1;
    r = query[i].r - 1;
    c = query[i].c;
    
  }
  return 0;
}
