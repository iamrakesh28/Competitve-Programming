#include <iostream>
#include <>
#include <list>

using namespace std;

const int N = 2e5;
int A[N], deg[N];
bool mark[N];
list <int> adj[N];

int main() {
  int n, root;
  scanf("%d", &n);
  for (int i = 0; i < n - 1; ++i) {
    scanf("%d", A + i);
    deg[A[i] - 1]++;
    mark[i] = false;
  }
  mark[n - 1] = false;
  root = A[0];
  return 0;
}
