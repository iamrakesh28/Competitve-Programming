#include <iostream>

using namespace std;

const int N = 17;
int a[1 << N], t[];

void build(int v, int tl, int tr) {
  if (tl == tr)
    t[v] = {a[tl], 0};
  else {
    int tm = (tl + tr) / 2;
    build(2 * v, tl, tm);
  }
}

int main() {
  int n, m;
  scanf("%d%d", &n, &m);
  for (int i = 0; i < (1 << n); ++i)
    scanf("%d", a + i);
  while (m--) {
    int p, b;
    scanf("%d%d", &p, &b);
  }
  return 0;
}
