#include <iostream>
#include <cmath>

typedef long long ll;
using namespace std;

ll gcd(ll a, ll b, ll &x, ll &y) {
  if (a == 0) {
    x = 0, y = 1;
    return b;
  }
  ll x1, y1, d;
  d = gcd(b % a, a, x1, y1);
  x = y1 - (b / a) * x1;
  y = x1;
  return d;
}

int main() {
  ll n, p, w, d, g, x, y, cx, cy, k, z;
  cin>>n>>p>>w>>d;
  g = gcd(w, d, x, y);
  if (p % g) {
    puts("-1");
    return 0;
  }
  x = x * (p / g);
  y = y * (p / g);
  cx = d / g;
  cy = w / g;
  if (x < 0 && y < 0)
    puts("-1");
  else if (x < 0) {
    k = ceil(-1.0 * x / cx);
    x = x + k * cx;
    y = y - k * cy;
    if (x >= 0 && y >= 0 && x + y <= n)
      cout<<x<<" "<<y<<" "<<(n - x - y)<<"\n";
    else
      puts("-1");
  }
  else if (y < 0) {
    k = ceil(-1.0 * y / cy);
    x = x - k * cx;
    y = y + k * cy;
    if (x >= 0 && y >= 0 && x + y <= n)
      cout<<x<<" "<<y<<" "<<(n - x - y)<<"\n";
    else
      puts("-1");
  }
  else {
    if (x + y <= n)
      cout<<x<<" "<<y<<" "<<(n - x - y)<<"\n";      
    else {
      if (cx > cy) {
	k = x / cx;
	x = x - k * cx;
	y = y + k * cy;
      }
      else {
	k = y / cy;
	x = x + k * cx;
	y = y - k * cy;
      }
      if (x + y <= n)
	cout<<x<<" "<<y<<" "<<(n - x - y)<<"\n";
      else
	puts("-1");
    }
  }
  return 0;
}
